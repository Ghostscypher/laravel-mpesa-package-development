{
    "name": "hackdelta/laravel-mpesa",
    "description": "Laravel library to interact with M-Pesa APIs",
    "type": "library",
    "keywords": [
        "mpesa",
        "hackdelta",
        "laravel-mpesa"
    ],
    "require": {
        "php": "^7.0|8.0",
        "laravel/framework": "^7.0|8.0",
        "illuminate/support": "^7.0|8.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "ghostscypher",
            "email": "bngetich69@gmail.com",
            "role": "developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Hackdelta\\LaravelMpesa\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hackdelta\\LaravelMpesa\\LaravelMpesaServiceProvider"
            ],
            "aliases": {
                "LaravelMpesa" : "Hackdelta\\LaravelMpesa\\MultitenancyFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
